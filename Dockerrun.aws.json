{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "jadenkim5179/nimble-server",
    "Update": "true"
  },
  "Environment": {
    "DATABASE_HOST": "$DATABASE_HOST",
    "DATABASE_USER": "$DATABASE_USER",
    "DATABASE_PASSWORD": "$DATABASE_PASSWORD",
    "DATABASE_NAME": "$DATABASE_NAME",
    "OAUTH_GOOGLE_ID": "$OAUTH_GOOGLE_ID",
    "OAUTH_GOOGLE_SECRET": "$OAUTH_GOOGLE_SECRET",
    "OAUTH_GOOGLE_REDIRECT_URL": "$OAUTH_GOOGLE_REDIRECT_URL",
    "OAUTH_NAVER_ID": "$OAUTH_NAVER_ID",
    "OAUTH_NAVER_SECRET": "$OAUTH_NAVER_SECRET",
    "OAUTH_NAVER_REDIRECT_URL": "$OAUTH_NAVER_REDIRECT_URL",
    "JWT_ACCESS_TOKEN_SECRET": "$JWT_ACCESS_TOKEN_SECRET",
    "JWT_REFRESH_TOKEN_SECRET": "$JWT_REFRESH_TOKEN_SECRET",
    "JWT_REFRESH_TOKEN_EXPIRATION_TIME": "$JWT_REFRESH_TOKEN_EXPIRATION_TIME",
    "JWT_ACCESS_TOKEN_EXPIRATION_TIME": "$JWT_ACCESS_TOKEN_EXPIRATION_TIME"
  },
  "Ports": [
    {
      "ContainerPort": 3001,
      "HostPort": 5000
    }
  ]
}
